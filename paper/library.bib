@article{Yu2008,
abstract = {For complex industrial process with multiple operating conditions, the traditional multivariate process monitoring techniques such as PCA and PLS are ill-suited because the fundamental assumption that the operating data follow a unimodal Gaussian distribution usually becomes invalid. In this article, a novel multimode process monitoring appraoch based on finite Gaussian mixture model and Bayesian inference strategy is proposed. First the process data are assumed to be from a number of different clusters, each of which corresponds to an operating mode and can be characterized by a Gaussian component. In the absense of a priori process knowledge, the Figueirdo-Jain algorithm is then adopted to automatically optimize the number of Gaussian components and estimate their statistical distribution parameters. With the obtained FGMM, a Bayesian inference strategy is further utilized to compute the posterior probabilities of each monitored sample belonging to the multiple components and derive an integrated global probablistic index for fault detection and multi-mode processes. The validity and effectiveness of the proposed monitoring approach are illustrated through three examples: 1. a simple multivariate linear system. 2. a simulated continuous stirred tank heater process, and 3. the Tenesse Eastman challenge problem. The comparison of monitoring results demonstrates that the proposed approach is superior to the conventioanl PCA method and can achieve accurate and early detection of various types of faults in multimode processes.},
author = {Yu, Jie and Qin, S Joe},
doi = {10.1002/aic},
file = {:Volumes/OSXData/Users/blu3ness/Documents/Mendeley Desktop/Yu, Qin - 2008 - Multimode Process Monitoring with Bayesian Inference-Based Finite Gaussian Mixture Models.pdf:pdf},
journal = {AIChE Journal},
keywords = {bayesian inference,correspondence concerning this article,eastman chemical process,fault detection,finite gaussian mixture,global probabilistic index,j,mahalanobis distance,model,multimode process monitoring,should be addressed to,tennessee,yu at this cur-},
number = {7},
pages = {1811--1829},
title = {{Multimode Process Monitoring with Bayesian Inference-Based Finite Gaussian Mixture Models}},
volume = {54},
