@article{Zeng2008,
abstract = {The objective of this work is the creation of preditive models that can forecast the electrical or physical parameters of wafers using data collected from the relevant processing tools. In this way, direct measurements from the wafer can be minimized or elminated altogether, hence the term "virtual" metrology. Challenges include the selection of the appropriate process step to monitor, the pre-treatment of the raw data, and the development of a Virtual Metrology Model (VMM) that can track a manufacturing process as it ages. A key step in any VM application is dimensionality reduction, i.e. ensuring that the proper subset of predictors is included in the model. In this paper, a software tool developed with MATLAB is demonstrated for interactive data prescreening and selection. This is combined with a variety of automated statistical techniques. These include step-wise regression and genetic selection in conjunction with linear modelling such as Principal Component Regression (PCR) and Partial Least Squares (PLS). Modelling results based on industrial data sets are also used to demonstrate the effectiveness of these methods.},
author = {Zeng, Dekong and Tan, Yajing and Spanos, Costas J.},
doi = {10.1117/12.772739},
file = {:Volumes/OSXData/Users/blu3ness/Documents/Mendeley Desktop/Zeng, Tan, Spanos - 2008 - Dimensionality reduction methods in virtual metrology.pdf:pdf},
issn = {0277786X},
journal = {Proceedings of SPIE},
keywords = {advanced process control,clustering,neural networks,pcr,pls,statistical modeling,virtual metrology},
pages = {692238--692238--11},
publisher = {Spie},
title = {{Dimensionality reduction methods in virtual metrology}},
url = {http://link.aip.org/link/PSISDG/v6922/i1/p692238/s1{\&}Agg=doi},
volume = {6922},
