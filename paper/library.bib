file = {:Volumes/OSXData/Users/blu3ness/Documents/Mendeley Desktop//Garcı, Kourti, Macgregor - 2003 - Troubleshooting of an Industrial Batch Process Using Multivariate.pdf:pdf;:Volumes/OSXData/Users/blu3ness/Documents/Mendeley Desktop//Garcı, Kourti, Macgregor - 2003 - Troubleshooting of an Industrial Batch Process Using Multivariate(2).pdf:pdf},
