@article{Tomasi2004a,
abstract = {Two different algorithms for time-alignment as a preprocessing step$\backslash$nin linear factor models are studied. Correlation optimized warping$\backslash$nand dynamic time warping are both presented in the literature as$\backslash$nmethods that can eliminate shift-related artifacts from measurements$\backslash$nby correcting a sample vector towards a reference. In this study$\backslash$nboth the theoretical properties and the practical implications of$\backslash$nusing signal warping as preprocessing for chromatographic data are$\backslash$ninvestigated. The connection between the two algorithms is also discussed.$\backslash$nThe findings are illustrated by means of a case study of principal$\backslash$ncomponent analysis on a real data set, including manifest retention$\backslash$ntime artifacts, of extracts from coffee samples stored under different$\backslash$npackaging conditions for varying storage times. We concluded that$\backslash$nfor the data presented here dynamic time warping with rigid slope$\backslash$nconstraints and correlation optimized warping are superior to unconstrained$\backslash$ndynamic time warping; both considerably simplify interpretation of$\backslash$nthe factor model results. Unconstrained dynamic time warping was$\backslash$nfound to be too flexible for this chromatographic data set, resulting$\backslash$nin an overcompensation of the observed shifts and suggesting the$\backslash$nunsuitability of this preprocessing method for this type of signals.$\backslash$nCopyright © 2004 John Wiley {\&} Sons, Ltd.},
author = {Tomasi, Giorgio and {Van Den Berg}, Frans and Andersson, Claus},
